+            // select the current row if there is no selection
+            auto indx = selModel->currentIndex();
+            if(indx.isValid()) {
+                selModel->select(indx, QItemSelectionModel::Select | QItemSelectionModel::Rows);
+                return;
+            }
+        }
+    }
+    // otherwise, view the selected files
+    viewSelectedFiles();
